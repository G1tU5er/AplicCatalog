@model IEnumerable<AplicCatalog.Models.AnScolar>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Adauga An", "Create")
</p>
<div style="height: 200px; overflow: auto;">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.InceputAnScolar)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SfarsitAnScolar)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.InceputAnScolar)
                 
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SfarsitAnScolar)
                </td>
                <td>
                    @Html.ActionLink("Modifica", "Edit", new { id = item.IdAn }) |
                    @Html.ActionLink("Sterge", "Delete", new { id = item.IdAn })
                </td>
            </tr>
        }

    </table>
    </div>
